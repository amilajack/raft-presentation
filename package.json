{
  "private": true,
  "name": "@raft/presentation",
  "description": "Summer 2018 Internship Presentation",
  "license": "MIT",
  "author": "Vikram Rangaraj <vik120@icloud.com> (https://github.com/vikr01)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vikr01/raft-presentation.git"
  },
  "bugs": "https://github.com/nutanix/vikr01/raft-presentation/issues",
  "version": "1.0.1",
  "main": "dist/server.js",
  "scripts": {
    "build": "yarn run clean && concurrently --raw \"yarn run build-pack\" \"yarn run compile\"",
    "build-pack": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.prod.babel.js --progress --colors",
    "clean": "rimraf 'dist'",
    "compile": "cross-env NODE_ENV=production babel src --out-dir dist --source-maps --copy-files",
    "dev": "cross-env NODE_ENV=development webpack-dashboard --color blue -- webpack-dev-server --config ./webpack/webpack.config.devserver.babel.js",
    "export": "spectacle-renderer --delay 3000",
    "lint": "eslint './**/*.*(js|jsx)'",
    "lint-fix": "yarn run lint --fix",
    "lint-styles": "stylelint --ignore-path .eslintignore '**/*.*(css|scss)' --syntax scss",
    "lint-styles-fix": "yarn run lint-styles --fix",
    "postlint-styles-fix": "prettier --ignore-path .eslintignore '**/*.*(css|scss|less)'",
    "prepare": "yarn run build",
    "start": "node dist/server.js",
    "test": "yarn run lint && #jest tests should go here",
    "webpack": "webpack"
  },
  "dependencies": {
    "dotenv": "^6.0.0",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.1",
    "@babel/core": "^7.0.0-rc.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0-rc.1",
    "@babel/plugin-proposal-decorators": "7.0.0-rc.1",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-rc.1",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-rc.1",
    "@babel/plugin-transform-react-constant-elements": "7.0.0-rc.1",
    "@babel/plugin-transform-react-inline-elements": "7.0.0-rc.1",
    "@babel/plugin-transform-runtime": "7.0.0-rc.1",
    "@babel/preset-env": "7.0.0-rc.1",
    "@babel/preset-flow": "^7.0.0-rc.1",
    "@babel/preset-react": "7.0.0-rc.1",
    "@babel/register": "^7.0.0-rc.1",
    "@babel/runtime": "7.0.0-rc.1",
    "@babel/runtime-corejs2": "^7.0.0-rc.1",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-flow-runtime": "^0.17.0",
    "babel-plugin-prism-import": "^1.0.0",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.14",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.6.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.7",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-formidable": "^3.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-security": "^1.4.0",
    "file-loader": "^1.1.5",
    "flow-bin": "^0.78.0",
    "flow-runtime": "^0.17.0",
    "flow-typed": "^2.5.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.0-rc.13",
    "is-buffer": "^1.1.5",
    "lint-staged": "^7.2.2",
    "markdown-loader": "^2.0.1",
    "moment": "^2.22.2",
    "node-libs-browser": "^2.0.0",
    "normalize.css": "^8.0.0",
    "prettier": "^1.14.2",
    "prettier-package-json": "^2.0.0",
    "prism-reactjs": "^1.2.2",
    "prop-types": "^15.6.2",
    "raw-loader": "^0.5.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^3.1.1",
    "react-router-dom": "^4.3.1",
    "react-transform-catch-errors": "^1.0.2",
    "redbox-react": "^1.5.0",
    "rimraf": "^2.6.2",
    "spectacle": "^5.0.0",
    "spectacle-code-slide": "^0.5.2",
    "spectacle-renderer": "^0.0.3",
    "style-loader": "^0.19.0",
    "stylelint": "^9.4.0",
    "stylelint-config-airbnb": "^0.0.0",
    "stylelint-config-prettier": "^4.0.0",
    "title-case": "^2.1.1",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^0.6.2",
    "webpack": "^4.16.5",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.4",
    "write-file-webpack-plugin": "^4.3.2",
    "yarn": "^1.9.4"
  },
  "engines": {
    "node": ">=8",
    "yarn": ">=1.9.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.*(js|jsx)": [
        "eslint --fix",
        "git add"
      ],
      "*.json|.*(babelrc|eslintrc|prettierrc|stylelintrc)": [
        "prettier --ignore-path .eslintignore --parser json --write",
        "git add"
      ],
      "*.*(css|scss)": [
        "stylelint --ignore-path .eslintignore --parser scss --fix",
        "prettier --ignore-path .eslintignore --write",
        "git add"
      ],
      "*.*(yml|less|md)": [
        "prettier --ignore-path .eslintignore --write",
        "git add"
      ],
      "package.json": [
        "prettier-package-json --write",
        "git add"
      ]
    }
  }
}
